<meta charset="UTF-8">

<script>

    function pulaLinha() {

        document.write("<br><br>");
    }

    function exibe(resultados){

        document.write(resultados); 
        pulaLinha();
    }

    var ano = 2024;

    exibe("Joaquim tem " + (ano - 2003) + " anos");

    exibe("Carlos tem " + (ano - 2005) + " anos");

    exibe("Beatriz tem " + (ano - 2000) + " anos");

</script>
