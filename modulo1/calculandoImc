<meta charset= "UTF-8">

<script>

    function pulaLinha(){

        document.write("<br>");
        document.write("<br>");

    }

    function mostra(frase){

        document.write(frase);
        pulaLinha();
    }

    function calculaImc(peso, altura){

        var imc = peso / (altura * altura);
        mostra("O imc calculado Ã© " + Math.round(imc));
    }

    calculaImc(58, 1.80);
    calculaImc(59, 1.66);

</script>
