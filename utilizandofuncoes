<script>

    function pulaLinha() {

        document.write("<br><br>")
    }

    var ano = 2024;

    document.write("Joaquim tem " + (ano - 2003) + " anos");
    pulaLinha();

    document.write("Carlos tem " + (ano - 2005) + " anos");

    pulaLinha();

    document.write("Beatriz tem " + (ano - 2000) + " anos");

</script>
